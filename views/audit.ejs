<!doctype html>
<html>
  <head>
    <title>Audit Log</title>
    <link rel="stylesheet"  type="text/css" href="/main.css" />
  </head>

  <body>
    <h1>Audit Log</h1>

    <ul>
      <% audit.forEach(function(entry) { %>
        <li>
          <strong><%= entry.username %></strong> —
          <%= entry.status %> —
          <%= entry.time %>
        </li>
      <% }); %>
    </ul>

    <p><a href="/">Back to home</a></p>
  </body>
</html>
